<!DOCTYPE html>
<html ng-app="Aktivator">
<head>
  <title>Backend</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav>
		<%= render 'layouts/navigation' %>
	</nav>
	<main role="main">
	  <div class="container">
	    <div class="content">
	      <div class="row">
	        <div class="span12">
	          <%= render 'layouts/messages' %>
	          <%= yield %>
	        </div>
	      </div>
	      <footer></footer>
	    </div>
	  </div>
	</main>

</body>
</html>
